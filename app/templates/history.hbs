<div class="w-100 d-flex justify-content-center">
  <BsNavbar @backgroundColor="white" as |navbar|>
    <navbar.content>
      <navbar.nav as |nav|>
        {{#each model as |repo|}}
        <nav.item>
          <div class="repo-nav"><nav.linkTo @route="history.service" @model={{repo.name}}>{{repo.name}}</nav.linkTo></div>
        </nav.item>
        {{/each}}
      </navbar.nav>
    </navbar.content>
  </BsNavbar>
</div>
{{outlet}}
